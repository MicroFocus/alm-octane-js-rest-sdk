{
  "name": "@microfocus/alm-octane-js-rest-sdk",
  "version": "16.1.100-1",
  "description": "NodeJS wrapper for the Micro Focus ALM Octane API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles routes/* octane.json ./dist/",
    "test": "npm run build && cd dist && mocha --recursive",
    "test-jenkins": "npm run build && cd dist && set JUNIT_REPORT_PATH=report.xml && set NODE_ENV=test && mocha --recursive --colors --reporter mocha-jenkins-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MicroFocus/alm-octane-js-rest-sdk.git"
  },
  "keywords": [
    "octane",
    "microfocus",
    "alm",
    "sdk",
    "rest"
  ],
  "contributors": [
    "DaSheng Jian <da-sheng.jian@microfocus.com>",
    "Spencer Bruce <spencer.bruce@microfocus.com",
    "Laura Andrada Buzas <laura-andrada.buzas@microfocus.com>",
    "Alexander Stanciu <alexander.stanciu@microfocus.com>",
    "Silviu-Constantin Anton <silviu-constantin.anton@microfocus.com>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.1.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.0",
    "@types/pluralize": "^0.0.29",
    "@types/request": "^2.48.8",
    "@types/tough-cookie": "^4.0.2",
    "apidoc": "0.19.0",
    "axios-mock-adapter": "1.20.0",
    "copyfiles": "^2.4.1",
    "jsdoc": "3.6.3",
    "mocha": "10.2.0",
    "mocha-jenkins-reporter": "0.4.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "async-mutex": "0.1.4",
    "axios": "0.26.1",
    "axios-cookiejar-support": "2.0.4",
    "command-line-args": "5.1.1",
    "debug": "3.1.0",
    "http-errors": "1.7.3",
    "log4js": "6.4.5",
    "pluralize": "3.0.0",
    "request": "2.87.0",
    "tough-cookie": "4.0.0"
  },
  "apidoc": {
    "title": "node-octane",
    "name": "node-octane",
    "template": {
      "withCompare": false
    }
  },
  "bugs": {
    "url": "https://github.com/MicroFocus/alm-octane-js-rest-sdk/issues"
  },
  "homepage": "https://github.com/MicroFocus/alm-octane-js-rest-sdk#readme",
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  }
}
